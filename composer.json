    "version": "0.1.1",
    "type": "wp-cli-package",
    "homepage": "https://github.com/cubetech/wpcli-sanitizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Remo Blaser",
            "email": "remo.blaser@cubetech.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
